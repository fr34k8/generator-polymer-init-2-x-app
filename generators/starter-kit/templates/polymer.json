{
  "entrypoint": "index.html",
  "shell": "src/<%= ns %>-app.html",
  "fragments": [
    "src/<%= ns %>-view1.html",
    "src/<%= ns %>-view2.html",
    "src/<%= ns %>-view3.html",
    "src/<%= ns %>-view404.html"
  ],
  "sourceGlobs": [
   "src/**/*",
   "images/**/*",
   "bower.json"
  ],
  "includeDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/webcomponents-lite.min.js"
  ]
}
